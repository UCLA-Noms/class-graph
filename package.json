{
  "name": "noms-class-graph",
  "version": "1.0.0",
  "private": true,
  "homepage": "https://ucla-noms.github.io/class-graph/",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "lint": "eslint \"**/*.tsx\" \"**/*.ts\" && npx stylelint \"**/*.css\" \"**/*.scss\" && prettier --check .",
    "lint-fix": "eslint --fix \"**/*.tsx\" \"**/*.ts\" && npx stylelint --fix \"**/*.css\" \"**/*.scss\" && prettier --write .",
    "pre-commit": "npm run lint-fix || true",
    "prepare": "husky install",
    "ci": "npm run lint",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/utilities": "^3.2.1",
    "@nextui-org/react": "^2.2.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint-config-prettier": "^8.8.0",
    "framer-motion": "^10.18.0",
    "normalize.css": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-graph-vis": "^1.0.7",
    "react-helmet-async": "^2.0.1",
    "react-zoom-pan-pinch": "^3.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@types/babel__core": "^7.20.0",
    "@types/babel__template": "^7.4.1",
    "@types/prettier": "^2.7.2",
    "@types/react": "^18.2.38",
    "@types/vis": "^4.21.27",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.40.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.1.0",
    "husky": "^8.0.3",
    "postcss": "^8.4.35",
    "prettier": "^2.8.8",
    "sass": "^1.69.5",
    "stylelint": "^15.6.1",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^33.0.0",
    "svgo": "^3.0.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.5",
    "vite-plugin-svgr": "^3.2.0",
    "vite-tsconfig-paths": "^4.2.0"
  }
}
